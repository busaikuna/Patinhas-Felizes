{
    "version": 2,
    "builds": [
      {
        "src": "static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/feed.html",
        "dest": "/static/html/feed.html"
      },
      {
        "src": "/search.html",
        "dest": "/static/html/search.html"
      },
      {
        "src": "/(.*)",
        "dest": "/static/$1"
      }
    ]
  }
  